// test/widget_test.dart
import 'package:flutter_test/flutter_test.dart';
import 'package:cinephoria_mobile/main.dart';

void main() {
  testWidgets('App affiche Cinéphoria', (WidgetTester tester) async {
    await tester.pumpWidget(const MyApp());
    expect(find.text('Cinéphoria'), findsOneWidget);
  });
}